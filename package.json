{
  "name": "nextjs-template-zjl",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -f .env.development -- next dev --turbo",
    "build:dev": "dotenvx run -f .env.development -- next build",
    "start:dev": "dotenvx run -f .env.development -- next start",
    "build:prod": "dotenvx run -f .env.production -- next build",
    "start:prod": "dotenvx run -f .env.production -- next start",
    "lint": "next lint",
    "format": "prettier --write src/",
    "db:migrate": "dotenvx run -f .env.development -- prisma migrate dev --name",
    "db:generate": "dotenvx run -f .env.development -- prisma generate",
    "db:deploy": "dotenvx run -f .env.production -- prisma migrate deploy && prisma generate",
    "db:push": "dotenvx run -f .env.development -- prisma db push && prisma generate",
    "db:seed": "dotenvx run -f .env.development -- tsx ./prisma/seed.ts",
    "db:wipe": "dotenvx run -f .env.development -- prisma migrate reset --force && prisma db push",
    "db:studio": "dotenvx run -f .env.development -- prisma studio"
  },
